#include <iostream>

using namespace std;

int main()
{
    int a;
    cin>>a;
    if(a==1){
        cout<<"January"<<endl;
        
    }
    
      else if(a==2){
        cout<<"February"<<endl;
        
    }
    
     else if(a==3){
        cout<<"March"<<endl;
        
    }
    
     else if(a==4){
        cout<<"April"<<endl;
        
    }
    
     else if(a==5){
        cout<<"May"<<endl;
        
    }
    
     else if(a==6){
        cout<<"Jun"<<endl;
        
    }
    
      else if(a==7){
        cout<<"July"<<endl;
        
    }
    
     else if(a==8){
        cout<<"August"<<endl;
        
    }
    
    else if(a==9){
        cout<<"September"<<endl;
        
    }
    
     else if(a==10){
        cout<<"Octobor"<<endl;
        
    }
    
      else if(a==11){
        cout<<"November"<<endl;
        
    }
    
     else if(a==12){
        cout<<"December"<<endl;
        
    }
    


    return 0;
}
